root = true

[*.cs]
charset = utf-8-bom
end_of_line = crlf
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.{csproj,sln}]
end_of_line = crlf
insert_final_newline = true

[*.{json,xml,yml,yaml,md}]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
